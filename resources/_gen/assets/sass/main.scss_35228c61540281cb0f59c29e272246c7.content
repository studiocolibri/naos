.list-projects__item {
  line-height: 0;
  position: relative;
  overflow: clip; }
  .list-projects__item:before {
    content: "+";
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: white;
    background: #DC0814;
    width: 40px;
    height: 40px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 32px;
    opacity: 0;
    transition: opacity 150ms ease-out;
    z-index: 2; }
  .list-projects__item:after {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: #DC0814;
    background: rgba(220, 8, 20, 0.3);
    opacity: 0;
    transition: opacity 150ms ease-out;
    z-index: 1; }
  .list-projects__item img {
    display: block;
    width: 100%;
    height: auto;
    transition: filter 150ms ease-out, transform 150ms ease-out; }
    .list-categories .list-projects__item img {
      aspect-ratio: 1284/1394;
      object-fit: cover; }
  .list-projects__item:hover, .list-projects__item:focus {
    outline: 0; }
    .list-projects__item:hover:before, .list-projects__item:focus:before {
      opacity: 1; }
    .list-projects__item:hover:after, .list-projects__item:focus:after {
      opacity: 1; }
    .list-projects__item:hover img, .list-projects__item:focus img {
      filter: blur(1px);
      transform: scale(1.1); }
    .list-projects__item:hover .list-projects__item-txt, .list-projects__item:focus .list-projects__item-txt {
      transform: translateY(5em); }

.list-projects__item-txt {
  position: absolute;
  bottom: 0;
  background: rgba(255, 255, 255, 0.8);
  width: 100%;
  margin: 0;
  padding: 0.55em;
  color: #484747;
  transition: transform 150ms ease-out; }
  .list-categories .list-projects__item-txt {
    font-weight: 400; }

